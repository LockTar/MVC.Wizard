      <dependency id="jQuery" version="1.10.2" />
      <dependency id="jQuery.Validation" version="1.11.1" />
      <dependency id="knockoutjs" version="3.0.0" />
      <dependency id="Knockout.Mapping" version="2.4.0" />
      <dependency id="Microsoft.AspNet.Mvc" version="5.0.0" />
      <dependency id="Microsoft.AspNet.Razor" version="3.0.0" />
      <dependency id="Microsoft.AspNet.WebPages" version="3.0.0" />
      <dependency id="Microsoft.Web.Infrastructure" version="1.0.0.0" />
    </dependencies>
    <references></references>
    <tags>MVC JSON Wizard Ajax jQuery jQueryValidation</tags>
  </metadata>
  <files>
